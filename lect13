
#include<iostream>
#include<vector>
using namespace std;

int main(){
    // Reverse an Array
    int n=5; 
    int arr[n] = {12, 13, 14, 15, 16};
    int newarr[n];
    int j=n-1;
    for(int i=0; i<n; i++){
        newarr[j]=arr[i];
        j--;
    }
    for(int i=0; i<n; i++){
        cout << newarr[i] << " ";
    } // In this we take an extra array so space complexity: n

    // Bubble Sort 
     vector<int> selectionSort(vector<int>& arr) {
	int n=arr.size();
	for(int i=0; i<n; i++){
		int index = i;
		for(int j=i; j<n; j++){
			if(arr[j] < arr[index]){
				index = j;
			}
		}
		swap(arr[index],arr[i]);
	}

	return arr;
  }
  // time complexity:n^2  
  // code write in strike practice


return 0;
}

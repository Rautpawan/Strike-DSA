#include <iostream>
using namespace std;

int main(){
    int n = 5;
    int arr[n] = {12, 4, 2, 10, 1};

// Bubble Sort  (solved in Strike)
    for(int j=0; j<n; j++){
    for(int i=1; i<n-j; i++){
        if(arr[i-1]>arr[i]){
            swap(arr[i-1],arr[i]);
        }    
    }
}
    for(int i=0; i<n; i++){
        cout << arr[i] <<" ";
    }
    cout << endl;

// Insertion Sort
for(int i=0; i<n; i++){
		for(int j=i; j>0; j--){
			if(arr[j]<arr[j-1]){
				swap(arr[j],arr[j-1]);
			}
			else{
				break;
			}
		}
	}
    for(int i=0; i<n; i++){
        cout << arr[i] <<" ";
    }


    // Binary search 
    int start=0 , end = n-1;
    int target;
    int nums[5];

		while(start<=end){

		int mid = (start+end)/2;

		if(nums[mid]==target){
			return mid;
		} else if(nums[mid]<target){
			start=mid+1;
		}else{
			end=mid-1;
		}
    }
	return -1;



return 0;
}

    //Reverse of array
    int arr[5]={23, 56, 73, 25, 12}
    void reverseArray(vector<int>& arr) {
    int n=arr.size();
	for(int i=0; i<n/2; i++){
		int temp=arr[i];
		arr[i]=arr[n-1-i];
		arr[n-1-i]=temp;
	}
}
